<div class="selection-area">
  <div class="header">
    <strong class="">
      Graphic Selection
    </strong>
    <a href="#" class="close-btn">
      <span class="glyphicon glyphicon-remove"></span>
    </a>
  </div>
  <div class="content-holder">
    <div class="clearfix sub-section">
      <label class="pull-left" >Upload a Logo</label>
      <div class="text-right ">
        <input type="button" value="NO" class="btn ">
        <input type="button" value="Yes" class="btn btn-danger">
      </div>
    </div>
    <div class="clearfix sub-section">
      <label class="title" >Search</label>

      <div class="form-holder">
        <input type="text" placeholder="Enter your Search..." class="form-control">
      </div>

    </div>
    <div class="clearfix sub-section">
      <label class="title" >Select</label>

      <div class="form-holder">
        <select  class="form-control">
          <option>Baseball</option>
          <option>knights</option>
          <option>knights</option>
        </select>
      </div>

    </div>
    <div class="color-selection">
      <strong class="heading">Select logo </strong>
      <ul class="logo-list">
        <%-graphics.each do |graphic|%>
          <li><a class="active" href="#" data-remote="true"><img src="<%=graphic.image.url%>" class="graphic-image" data-image="<%=graphic.image%>"></a><span class="tile-view-text">front</span></li>
        <%end%>
      </ul>
    </div>
  </div>
  <div class="footer">
    <input type="button" value="Cancel" class="btn highlight graphics">
    <input type="button" value="Accept" class="btn btn-danger highlight graphics", data-graphics="true">
  </div>
</div>