<%=simple_form_for Logo.new, url: logos_path, method: :post do |f|%>

    <li class="form-group">
      <label class="title" >Upload Graphic</label>
      <div class="form-holder">
        <%=f.input_field :image, type: :file, multiple: true, class: "form-control", id:"img_inpput", data: {placement: current_user.try(:placement_pos)}%>
      </div>
    </li>

    <li class="form-group">
      <label class="title" >Select Logo</label>
      <div class="form-holder">
        <%= link_to((image_tag "", id: "set_image"), "#", class: "form-control", id:"graphic_selection", data:{style: params[:style_id],placement: current_user.try(:placement_pos)}, remote: true)%>
        <%= f.input_field :image, class: "form-control hide-sidebar" %>
      </div>
    </li>

    <div data-logo-colors></div>
    <li class="form-group">
      <div class="graphic-area hide-sidebar" id="sidebar-4">
        <%=render partial: "logo_selection", locals: {logos: logos, placement: placement}%>
      </div>
    </li>
    <div class="footer">
      <%= f.button :button, "Cancel", type: :reset, class: "btn highlight graphics", data: {graphics: "false"} %>
      <%=f.button :submit,  "Complete", class: "btn btn-danger highlight graphics", data: {graphics: "true"}, id: "logo_form", remote: true%>
    </div>
<%end%>
<script>
    var text_value = $('#text_team_number').val();
    document.getElementById('PL10_Text_Back').textContent = text_value;

</script>